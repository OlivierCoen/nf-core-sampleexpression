nextflow_process {

    name "Test Process EXPRESSIONATLAS_GETDATA"
    script "modules/local/expressionatlas/getdata/main.nf"
    process "EXPRESSIONATLAS_GETDATA"
    tag "getdata"

    test("Should run without failures") {

        when {

            process {
                """
                input[0] = "E-MTAB-552"
                """
            }
